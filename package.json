{
  "name": "create-react-fluig",
  "version": "2.0.1",
  "description": "CLI para criar projetos React integrados ao Fluig.",
  "license": "MIT",
  "type": "module",
  "bin": {
    "create-react-fluig": "./bin/create-react-fluig.js"
  },
  "files": [
    "bin",
    "template",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "fluig",
    "react",
    "cli",
    "create-react-fluig",
    "boilerplate"
  ],
  "scripts": {
