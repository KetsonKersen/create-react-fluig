    "build": "npm --workspace=packages/react run build"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.2",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
