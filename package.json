{
  "name": "react-fluig",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:api dev:front",
    "dev:api": "npm --workspace=packages/api run dev",
    "dev:front": "npm --workspace=packages/react run dev",
    "build": "npm --workspace=packages/react run build"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "bin": {
    "create-react-fluig": "bin/create-react-fluig.js"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.2",
    "prompts": "^2.4.2"
  }
}
